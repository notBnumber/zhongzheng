.container{height:100vh;background-color:#F7F8F8;display:flex;flex-direction:column}.container .city{padding:25rpx 20rpx;margin-bottom:15rpx;background-color:#fff}.container .city .title{font-size:28rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#393939}.container .city .position{font-size:30rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#FE203F;display:flex;align-items:center;margin-top:10rpx}.container .city .position image{width:28rpx;height:32rpx;margin-right:10rpx}.container .scroll{height:100%;overflow:hidden;background-color:#fff;position:relative}.container .scroll .box .title{background-color:#DFFEF1;height:50rpx;line-height:50rpx;font-size:28rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#222222;padding:0 20rpx;position:sticky;top:0}.container .scroll .box .itemBox{padding:0 20rpx}.container .scroll .box .itemBox .items{height:85rpx;line-height:85rpx;border-bottom:1px solid #E4E4E4;font-size:28rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#373737}.container .scroll .box .itemBox .items:last-of-type{border:none}.container .scroll .right{position:absolute;width:50rpx;right:10rpx;top:20rpx;text-align:center}.container .scroll .right view{font-size:28rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#474747;line-height:35rpx}
