.container {
  min-height: 100vh;
  position: relative;
}
.container .tabs {
  position: absolute;
  z-index: 999;
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .tabs .title {
  text-align: center;
  font-size: 36rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #fff;
  flex: 1;
  height: 88rpx;
  line-height: 88rpx;
}
.container .tabs image {
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
}
.container .tabs .white {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.container .my-details {
  position: relative;
  height: 400rpx;
  z-index: 9;
}
.container .my-details .img {
  width: 100%;
  height: 400rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 0;
}
.container .my-details .details {
  padding-top: 150rpx;
  position: absolute;
  width: 100%;
}
.container .my-details .details .balance {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 39rpx;
}
.container .my-details .details .balance test {
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #FFFFFF;
}
.container .my-details .details .balance image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 30rpx;
}
.container .my-details .details .withdrawal {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 99rpx;
}
.container .my-details .details .withdrawal .left {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .my-details .details .withdrawal .left text {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #f2f2f2;
}
.container .my-details .details .withdrawal .left view {
  margin-top: 50rpx;
  font-size: 50rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: #ffffff;
}
.container .my-details .details .withdrawal .right {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .my-details .details .withdrawal .right text {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #f2f2f2;
}
.container .my-details .details .withdrawal .right view {
  margin-top: 50rpx;
  font-size: 50rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: #ffffff;
}
.container .ul {
  background-color: #fff;
  padding: 0 33rpx;
  padding-top: 50rpx;
}
.container .ul .view {
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: #2d2d2d;
}
.container .ul .text {
  margin-top: 24rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #313131;
}
.container .btn {
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 12rpx;
  width: 626rpx;
  height: 84rpx;
  background: linear-gradient(-90deg, #38c6a0 0%, #6de0c3 100%);
  border-radius: 42rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #ffffff;
}
