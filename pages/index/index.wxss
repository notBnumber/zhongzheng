.container {
  width: 100%;
  padding-bottom: 47rpx;

}

.input-placeholder {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(186, 186, 186, 1);
}

.search {
  font-size: 26rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 17rpx 29rpx 17rpx 54rpx ;
}
.inp {
  position: relative;
}
.inp image {
  width: 30rpx;
  height: 31rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  left: 23rpx;
}
.search input {
  
  width: 536rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 1);
  border: 2rpx solid rgba(197, 197, 197, 1);
  border-radius: 28rpx;
  padding-left: 67rpx;
  box-sizing: border-box;
}

.searchLeft {
  display: flex;
  align-items: center;
}

.searchLeft image {
  width: 22rpx;
  height: 33rpx;
}

.searchLeft view {
  margin-left: 13rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
}

.swiper {
  width: 100%;
  height: 390rpx;

}

.swiper swiper {
  width: 100%;
  height: 100%;
}

swiper-item {
  width: 100%;
  height: 100%;
  position: relative;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.ind {
  width: 106rpx;
  height: 44rpx;
  position: absolute;
  right: 14rpx;
  bottom: 10rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);

  background: rgba(0, 0, 0, 1);
  opacity: 0.42;
  border-radius: 10rpx;
}

/* tab栏目 */
.oneTab {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 30rpx 24rpx 0 24rpx;
}

.oneTabItem {
  width: 340rpx;
  height: 190rpx;
}

.oneTabItemImg {
  width: 100%;
}

/* twoTab栏目 */
.twoTab {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 31rpx 45rpx 28rpx 45rpx;
}

.twoTabItem {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 25%;
}

.twoTabItemImg {
  width: 100rpx;
  height: 100rpx;

}

.twoTabItemName {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 19rpx;
}

/* 文字轮播 */
.word {
  width: 100%;
  height: 86rpx;
  padding-left: 25rpx;
  display: flex;
  align-items: center;
  border-top: 2rpx solid #f7f7f8;
}

.wordLeft {
  width: 63rpx;
  height: 38rpx;
  border: 1rpx solid rgba(51, 195, 157, 1);
  opacity: 0.99;
  border-radius: 5rpx;

  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 195, 157, 1);

}

.wordRight {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(160, 160, 160, 1);
  margin-left: 20rpx;
}

.break {
  width: 100%;
  height: 16rpx;
  background-color: #f7f8f8;

}

/* 热门推荐 */
.hot {
  width: 100%;
}

.hotTitle {
  padding-top: 17rpx;
}

.hotTitleImg {
  width: 35rpx;
  height: 47rpx;
  margin-right: 8rpx;
}

.hr {
  font-size: 40rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(13, 13, 13, 1);
}

.hotTab {
  padding: 26rpx 94rpx;
  display: flex;
  justify-content: space-between;
}

.hotTabItem {
  width: 133rpx;
  height: 50rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(211, 211, 211, 1);
  border-radius: 25rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(151, 151, 151, 1);
}

.active {
  border: 1px solid #04CB95;
  color: #04CB95;
}

.hotContent {
  padding: 0 20rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.hotContentItem {
  width: 345rpx;
  border-radius: 10rpx;
  overflow: hidden;
  box-shadow: 0rpx 3rpx 7rpx 0rpx rgba(151, 155, 157, 0.15);
  margin-bottom: 21rpx;
  padding-bottom: 21rpx;
}

.hotContentItemImg {
  width: 345rpx;
  height: 282rpx;

  /* border-radius:10rpx 10rpx 0rpx 0rpx; */
}

.hotContentItem .name {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(41, 41, 41, 1);
  margin-top: 23rpx;
  padding-left:10rpx;

}

.hotContentItem .address {
  margin-top: 20rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(164, 164, 164, 1);
  padding-left:10rpx;

}

.biaoqianList {
  display: flex;
  margin-top: 19rpx;
}

.yongjin {
  margin-top: 27rpx;
  display: flex;
  align-items: center;
  padding-left: 10rpx;

  

  
}
.yongjinLeft {
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(101, 101, 101, 1);
}

.yongjinRight {
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
