.container {
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.container .icon {
  margin-top: 140rpx;
  width: 221rpx;
  height: 161rpx;
}
.container .comment {
  margin-top: 39rpx;
  color: #a2a2a2;
  font-size: 30rpx;
}
.container .btn {
  margin-top: 112rpx;
  width: 400rpx;
  height: 72rpx;
  line-height: 72rpx;
  background: linear-gradient(-90deg, #38c6a0 0%, #6de0c3 100%);
  border-radius: 36rpx;
  font-size: 34rpx;
  text-align: center;
}
.container .proup {
  z-index: 99;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100vh;
  overflow: hidden;
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .proup .box {
  text-align: center;
}
.container .proup .box image.errIcon {
  width: 56rpx;
  height: 56rpx;
  margin-top: 30rpx;
}
.container .proup .box .Exhibition {
  background-color: #fff;
  width: 590rpx;
  height: 694rpx;
  border-radius: 10rpx;
  text-align: center;
}
.container .proup .box .Exhibition image {
  width: 373rpx;
  height: 373rpx;
  margin-top: 40rpx;
}
.container .proup .box .Exhibition .white {
  width: 407rpx;
  height: 36rpx;
  background-color: #EEEEEE;
  border-radius: 50%;
  margin: 0 auto;
}
.container .proup .box .Exhibition .text {
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #383838;
  margin-top: 30rpx;
}
.container .proup .box .Exhibition .btn {
  width: 300rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: linear-gradient(-90deg, #38c6a0 0%, #6de0c3 100%);
  border-radius: 42rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #FFFFFF;
  margin: 40rpx auto 0;
  border: none;
}
.teamWork {
  min-height: 100vh;
  background: #F7F8F8;
}
.teamWork .captain {
  padding: 46rpx 11rpx 0 10rpx;
  width: 729rpx;
}
.teamWork .captain .topic {
  padding-left: 27rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #000000;
}
.teamWork .captain .detail {
  margin-top: 25rpx;
  margin-bottom: 13rpx;
  width: 100%;
  height: 154rpx;
  background: #ffffff;
  box-shadow: 0px 4rpx 6rpx 0px rgba(190, 198, 205, 0.06);
  border-radius: 10rpx;
  padding: 28rpx 27rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.teamWork .captain .detail .avatar {
  width: 250rpx;
  display: flex;
  align-items: center;
}
.teamWork .captain .detail .avatar .gallary {
  width: 98rpx;
  height: 98rpx;
  border-radius: 49rpx;
}
.teamWork .captain .detail .avatar .name {
  padding-left: 24rpx;
  color: #000000;
  font-size: 32rpx;
}
.teamWork .captain .detail .phone {
  color: #515151;
  font-size: 28rpx;
}
.teamWork .teamer {
  padding: 75rpx 11rpx 0 10rpx;
  width: 729rpx;
}
.teamWork .teamer .title {
  padding-left: 27rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #000000;
}
.teamWork .teamer .inner {
  margin-top: 25rpx;
  margin-bottom: 14rpx;
  width: 100%;
  height: 154rpx;
  background: #ffffff;
  box-shadow: 0px 4rpx 6rpx 0px rgba(190, 198, 205, 0.06);
  border-radius: 10rpx;
  padding: 28rpx 27rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.teamWork .teamer .inner .contain {
  width: 283rpx;
  display: flex;
  align-items: center;
}
.teamWork .teamer .inner .contain .icon {
  width: 98rpx;
  height: 98rpx;
  border-radius: 49rpx;
}
.teamWork .teamer .inner .contain .describe {
  padding-left: 24rpx;
}
.teamWork .teamer .inner .contain .describe > view:first-of-type {
  color: #000000;
  font-size: 32rpx;
}
.teamWork .teamer .inner .contain .describe > view:last-of-type {
  margin-top: 20rpx;
  color: #515151;
  font-size: 28rpx;
}
.teamWork .teamer .inner .count {
  font-size: 28rpx;
  color: #515151;
}
