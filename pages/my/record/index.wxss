.container{min-height:100vh;background-color:#F7F8F8}.container .nothing{height:100vh;background-color:#fff;display:flex;justify-content:center;align-items:center}.container .nothing .box{font-size:30rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#A2A2A2;text-align:center}.container .nothing .box image{width:221rpx;height:161rpx;display:block;margin-bottom:30rpx}.container .recordList{padding:18rpx 15rpx 0}.container .recordList .items{margin-bottom:16rpx;height:140rpx;background-color:#fff;box-shadow:0px 4rpx 6rpx 0px rgba(0,0,0,0.06);border-radius:10rpx;display:flex;justify-content:space-between;align-items:center;padding:0 20rpx}.container .recordList .items .left view:first-child{font-size:30rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#000000}.container .recordList .items .left view:last-child{font-size:24rpx;font-family:PingFang-SC-Regular;font-weight:400;color:#666666;margin-top:10rpx}.container .recordList .items .right{text-align:right}.container .recordList .items .right view:first-child{font-size:30rpx;font-family:PingFang-SC-Regular;font-weight:bold;color:#000000}.container .recordList .items .right view:last-child{font-size:26rpx;font-family:PingFang-SC-Medium;font-weight:500;color:#666666;margin-top:10rpx}.container .recordList .items .green view:first-child{color:#22A864}.container .recordList .items .green view:last-child{color:#22A864}.container .recordList .items .red view:first-child{color:#FF536A}.container .recordList .items .red view:last-child{color:#FF2020}
